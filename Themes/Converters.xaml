<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Converters="clr-namespace:WpfAppTemplate.Converters">

    <Converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <Converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <Converters:NullOrEmptyStringToVisibilityConverter x:Key="NullOrEmptyStringToVisibilityConverter" />
    <Converters:ToUpperConverter x:Key="ToUpperConverter" />
    <Converters:ToLowerConverter x:Key="ToLowerConverter" />
    <Converters:EmptyListVisibilityConverter x:Key="EmptyListToVisibilityConverter" />
    <Converters:StringToBooleanConverter x:Key="StringToBooleanConverter" />
    <Converters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    <Converters:BooleanAndMultiConverter x:Key="BooleanAndMultiConverter" />
    <Converters:ObjectExistsToBooleanConverter x:Key="ObjectExistsToBooleanConverter" />

</ResourceDictionary>